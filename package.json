{
  "name": "notes-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://notes.rxluz.dev",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "detect-browser": "^5.0.0",
    "detect-browser-language": "0.0.2",
    "is-mobile": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-keyboard-shortcuts": "^1.1.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-tippy": "^1.3.4",
    "react-toggle": "^4.1.1",
    "redux": "^4.0.5",
    "redux-i18n": "^1.5.19",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^7.0.3"
  },
  "scripts": {
    "predeploy": "npm run build && npm run build-storybook",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "build-storybook": "build-storybook -c .storybook -o build/storybook",
    "eject": "react-scripts eject",
    "translation:extract": "i18n_extract --pattern=translate --source=src --locales=src/common/locales",
    "translation:import": "i18n_import --translations=src/common/locales --locales=src/common/locales",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "gh-pages": "^2.2.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8"
  }
}
